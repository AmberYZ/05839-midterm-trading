{
  "name": "d3-simple-slider",
  "version": "0.2.0",
  "description": "Renders an SVG slider",
  "keywords": [
    "d3",
    "d3-module",
    "slider",
    "visualization"
  ],
  "homepage": "https://github.com/johnwalley/d3-simple-slider",
